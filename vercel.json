{
  "version": 2,
  "builds": [
    {
      "src": "index.php",
      "use": "@vercel/php"
    },
    {
      "src": "get_guestbook.php",
      "use": "@vercel/php"
    },
    {
      "src": "save_guestbook.php",
      "use": "@vercel/php"
    },
    {
      "src": "save_rsvp.php",
      "use": "@vercel/php"
    },
    {
      "src": "css/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "js/script.js",
      "use": "@vercel/static"
    },
    {
      "src": "img/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}
